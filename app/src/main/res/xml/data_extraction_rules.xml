<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="file" path="sensitive/" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow all data for device transfer -->
        <include domain="file" path="." />
    </device-transfer>
</data-extraction-rules>